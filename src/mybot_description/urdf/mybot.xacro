<?xml version='1.0'?>

<robot name="FOURWHEElBOT" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find mybot_description)/urdf/mybot.gazebo" />


<!-- CHASSIS -->


  <link name="chassis">
    <pose>0 0 0.1 0 0 0</pose>
    <inertial>
      <mass value="15.0"/>
      <origin xyz="0 0 0.15" rpy=" 0 0 0"/>
      <inertia
          ixx="0.09" ixy="0" ixz="0"
          iyy="0.09" iyz="0"
          izz="0.09" />
    </inertial>
    <collision name='collision'>
     <geometry>
        <box size="1.0 1.0 .1"/>
      </geometry>
    </collision>
    <visual name='chassis_visual'>
      <origin xyz="0 0 0.15" rpy=" 0 0 0"/>
      <geometry>
        <box size="1.0 1.0 .1"/>
      </geometry>
    </visual>
  </link>


<!-- WHEEL LINKS -->


  <link name="left_wheel">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.05"/>
      <inertia
        ixx="0.051" ixy="0.0" ixz="0.0"
        iyy="0.051" iyz="0.0"
        izz="0.1"/>
    </inertial>
  </link>


  <link name="right_wheel">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.05"/>
      <inertia
        ixx="0.051" ixy="0.0" ixz="0.0"
        iyy="0.051" iyz="0.0"
        izz="0.1"/>
    </inertial>
  </link>


  <link name="left_back_wheel">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </collision>
    <visual name="left_wheel_back_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.05"/>
      <inertia
        ixx="0.051" ixy="0.0" ixz="0.0"
        iyy="0.051" iyz="0.0"
        izz="0.1"/>
    </inertial>
  </link>


  <link name="right_back_wheel">
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </collision>
    <visual name="right_wheel_back_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.05"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.05"/>
      <inertia
        ixx="0.051" ixy="0.0" ixz="0.0"
        iyy="0.051" iyz="0.0"
        izz="0.1"/>
    </inertial>
  </link>


<!-- WHEEL JOINTS -->



  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0.4 0.525 0.15" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1.0"/>
  </joint>


  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0.4 -0.525 0.15" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1.0"/>
  </joint>


 <joint type="continuous" name="left_wheel_back_hinge">
    <origin xyz="-0.4 0.525 0.15" rpy="0 0 0"/>
    <child link="left_back_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1.0"/>
  </joint>


  <joint type="continuous" name="right_wheel_back_hinge">
    <origin xyz="-0.4 -0.525 0.15" rpy="0 0 0"/>
    <child link="right_back_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1.0"/>
  </joint>


</robot>
